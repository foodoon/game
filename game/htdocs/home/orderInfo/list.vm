               <td> $!f.gmtModify</td>
               <td> $!f.gmtCreate</td>
          <td><a href="$!homeServer.getURI("orderInfo/edit.htm").addQueryData('id',"$!f.id")" >编辑</a></td>
    <td><a href="$!homeServer.getURI("orderInfo/detail.htm").addQueryData('id',"$!f.id")" >查看详情</a></td>
