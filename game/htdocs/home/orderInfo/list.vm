<a  class="btn btn-default"  href="$!homeServer.getURI("orderInfo/create.htm")" >新建</a>

<table  class="table">
 <tr>
              id
              goodsId
              userId
              leaveMsg
              isDeleted
              gmtModify
              gmtCreate
          <td>操作</td>
 </tr>
#foreach($f in $orderInfoDOList)
  <tr>
               <td> $!f.id</td>
               <td> $!f.goodsId</td>
               <td> $!f.userId</td>
               <td> $!f.leaveMsg</td>
